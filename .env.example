# Qdrant configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your-qdrant-api-key # Generate with: openssl rand -base64 24
QDRANT_READ_ONLY_API_KEY=your-qdrant-read-only-api-key # Generate with: openssl rand -base64 24
QDRANT_JWT_RBAC=false
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_DIST_PORT=6335
QDRANT_LOG_LEVEL=INFO
QDRANT_SERVICE_ENABLE_TLS=false
QDRANT_TLS_CERT=./tls/cert.pem
QDRANT_TLS_KEY=./tls/key.pem
# Collection name
COLLECTION_NAME=documentation

# Azure OpenAI configuration
AZURE_OPENAI_ENDPOINT=https://your-azure-openai-endpoint.openai.azure.com/
AZURE_OPENAI_KEY=your-azure-openai-key
AZURE_OPENAI_DEPLOYMENT=your-gpt-4o-mini-deployment
AZURE_OPENAI_API_VERSION=2025-01-01-preview
EMBEDDING_MODEL=your-text-embedding-ada-002-deployment

# OpenAI configuration
OPENAI_API_KEY=your-openai-api-key
